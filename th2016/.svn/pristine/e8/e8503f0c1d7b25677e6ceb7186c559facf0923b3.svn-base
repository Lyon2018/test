/* 主体不同样式部分 */
main .main{height:560px;border: 1px solid #d2d2d2;box-sizing: border-box;padding:30px;}
main .main .register{float: left;width:640px;border-right:1px solid #ccc;}
main .main .register_inp{margin:28px 0;overflow: hidden;}
main .main .register_inp p{float: left;line-height: 32px;font: 14px/32px"";}
main .main .register_inp p span{color:#ED3838;font:18px;line-height: 32px;}
main .main .register_inp div{float: left;width:274px;height: 32px;border:1px solid #ccc;line-height: 32px;text-align: center;}
main .main .register_inp div input{border:none;outline: none;width:220px;}
main .main .register_inp>span{float: left;width:180px;height: 32px;font:12px/32px"";margin-left:4px;background:#efefef;border: 1px solid #ccc;}
main .main .yzCode{overflow: hidden;}
main .main .yzCode p{float:left;}
main .main .yzCode p span{color:#ED3838;font:18px;line-height: 32px;}
main .main .yzCode div{float: left;width:114px;height: 32px;border:1px solid #ccc;line-height: 32px;text-align: center;}
main .main .yzCode div input{width:110px;border: none;outline: none;}
main .main .yzCode b{float: left;width:110px;height: 32px;background:#EFECEC;font:600 24px/32px"";text-align: center;}
main .main .yzCode>span{float: left;width:100px;height: 32px;font:12px/32px"";margin-left:4px;background:#efefef;border: 1px solid #ccc;}
main .main .yzCode
main .main .submit{padding-left:100px;}
main .main .submit div{width:420px;height: 32px;font:14px/32px"";margin-top:12px;}
main .main .submit div>input{vertical-align:text-bottom;margin-right: 6px;}
main .main .submit>input{width:230px;height: 34px;font:18px/32px"";outline:none;border: none;border-radius: 3px;box-shadow: 0 0 6px #000; margin-top:12px;text-align: center;background:#e2393c;color:#fff;}
.disHide{display: none;}
main .main .direct_login{height:425px;}
main .main .direct_login .direct_login_con{float:right;height:83px;width:230px;border-bottom:1px solid #ccc;display: flex;flex-direction: column;align-items: center;}
main .main .direct_login .direct_login_con span{font:12px/32px"";}
main .main .direct_login .direct_login_con button{width:136px;height: 32px;border-radius:4px; background:#f3f1f2;border:1px solid #ccc;text-align: center;}
main .main .direct_login .direct_login_con button a{font:16px/32px"";color:#F08080;}